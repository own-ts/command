{
  "name": "@own-js/command",
  "version": "1.0.4",
  "description": "A lightweight, high-performance, and strictly typed command-line framework.",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "devDependencies": {
    "@types/bun": "latest",
    "tsup": "^8.5.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "scripts": {
    "build": "tsup ./index.ts --format esm,cjs --dts --clean --minify",
    "push": "tsup ./index.ts --format esm,cjs --dts --clean --minify && bun publish  --access public"
  },
  "keywords": [
    "command",
    "parser",
    "typescript",
    "command-line",
    "subcommands",
    "cli-framework",
    "type-safe",
    "cobra",
    "command-line-interface",
    "bun",
    "deno",
    "nodejs",
    "zero-dependencies",
    "runtime-agnostic"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/own-js-org/command"
  },
  "bugs": {
    "url": "https://github.com/own-js-org/command/issues"
  },
  "author": {
    "name": "powerpuffpenguin"
  },
  "homepage": "https://github.com/own-js-org/command#readme",
  "license": "MIT"
}